<!DOCTYPE html>
<html lang="en">

<head>
    <title><%= title %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel='stylesheet' href='/stylesheets/styleIndex1.css' />
    <link rel="stylesheet" type="text/css" href="/dist/semantic.min.css">
    <script src="https://code.jquery.com/jquery-3.1.1.min.js"
        integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
    <script src="/dist/semantic.min.js"></script>

    <!-- BOOTSTRAP 4.4.1 -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
        integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

    <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.19.0/axios.js"></script>
</head>

<body>

    <section class="central">
        <nav class="navbar navbar-expand-sm navbar-light ">
            <div class="container-fluid ">
                <div class="nav_logo ">
                    <a class="navbar-brand " href="http://localhost:3000/0">
                        <!-- <img src="/images/pokemon-go-icon.png" alt="" class="imgNav"> -->
                        <img src="https://i2.wp.com/i.imgur.com/JRSTIux.jpg?resize=816%2C9999&ssl=1" class="imgNav">
                    </a>
                </div>

                <button class="navbar-toggler" type="button" data-toggle="collapse"
                    data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                    aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse navBarOpciones" id="navbarSupportedContent">
                    <ul class="navbar-nav ">
                        <li class="nav-item active"><a class="nav-link" id="inicio"
                                href="http://localhost:3000/0">Inicio</i></a></li>

                        <li class="nav-item"><a class="nav-link" id="jobs" href="#">Consultar Jobs</a class="nav-link">
                        </li>

                        <li class="nav-item"><a class="nav-link" id="listaRegistrados" href="#">Usuarios Registrados</a
                                class="nav-link"></li>

                    </ul>
                </div>

                <div id="usuario" class="dropdown navbar-right">
                    <button aria-expanded="false" aria-haspopup="true" class="btn dropdown-toggle" data-toggle="dropdown" type="button">
                        <img src="/images/guess.jpg" alt="" class="imgUsuario">
                        <span class="caret"></span>
                    </button>
                    <ul aria-labelledby="dropdownMenu" class="dropdown-menu">
                        <li>Salir</li>
                    </ul>
                </div>
            </div>
        </nav>

    </section>



    <section>
        <div class="ui middle aligned center aligned grid acortar">
            <div class="column">
                <h2 class="ui teal image header">
                <img src="https://i2.wp.com/i.imgur.com/JRSTIux.jpg?resize=816%2C9999&ssl=1" class="image">
                <div class="content">
                    Log-in to your account
                </div>
                </h2>
    
                <form class="ui large form" id="login">
                    <div class="ui stacked segment">
                        <div class="field">
                        <div class="ui left icon input">
                            <i class="user icon"></i>
                            <input type="text" name="email" placeholder="E-mail address" id="log-mail">
                        </div>
                        </div>
                        <div class="field">
                        <div class="ui left icon input">
                            <i class="lock icon"></i>
                            <input type="password" name="password" placeholder="Password" id="log-pass">
                        </div>
                        </div>
                        <button class="ui fluid large teal submit button" type="submit">Login</button>
                    </div>        
                </form>
                
                <div class="ui error message" id="login-error"></div>
            </div>
           
        </div>
    </section>

    <script type="module" src="/actions/index1.js"></script>
    <script type="module" src="/actions/login.js"></script>
    <script type="module" src="/actions/navBar.js"></script>
</body>

</html>